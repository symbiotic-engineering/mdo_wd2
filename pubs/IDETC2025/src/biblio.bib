groups     = {WEC PTO, Optimization, Controls},
keywords   = {Bypass valve, Control co-design, Levelised cost of energy, Oscillating-water-column, Wave energy, Wells turbine},
publisher  = {Elsevier BV},
readstatus = {skimmed},
}

