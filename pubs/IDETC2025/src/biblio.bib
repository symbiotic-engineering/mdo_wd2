doi        = {10.1016/j.oceaneng.2022.110863},
file       = {:Lit/wec optimization w mpc.pdf:PDF},
groups     = {Arrays, Controls},
keywords   = {Wave energy, Point-absorber arrays, Model-predictive control with convex formulation, Waveâ•ﬁbody interactions, Reactive power},
publisher  = {Elsevier BV},
readstatus = {skimmed},
}

